{
	"name": "vazha-sense",
	"displayName": "vazha Sense",
	"description": "Autocomplete for vazha Script",
	"version": "1.1.0",
	"publisher": "user9747",
	"engines": {
		"vscode": "^1.44.0"
	},
	"categories": [
		"Language Packs"
	],
	"keywords":["vazha","malayalam","manglish","malayalam python"],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"languages": [
			{
				"id": "vazha",
				"extensions": [
					".vazha"
				]
			}
		],
		"snippets": [
			{
				"language": "vazha",
				"path": "./snippet.json"
			}
		],
		"grammars": [
            {
                "language": "vazha",
                "scopeName": "source.vazha",
                "path": "./vazha.tmLanguage.json"
            }
        ]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.44.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.26.0",
		"@typescript-eslint/eslint-plugin": "^2.26.0",
		"glob": "^7.1.6",
		"mocha": "^10.2.0",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}